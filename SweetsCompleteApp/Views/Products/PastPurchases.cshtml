@model IEnumerable<SweetsCompleteApp.fixed_purchases>

@{
    ViewBag.Title = "PastPurchases";
}

<h2>PastPurchases</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.trans_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sale_price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.member.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.product.sku)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.trans_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sale_price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.member.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.product.sku)
        </td>
    </tr>
}

</table>
